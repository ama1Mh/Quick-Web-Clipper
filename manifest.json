{
  "manifest_version": 3,
  "name": "Quick Web Clipper",
  "version": "1.0",
  "description": "Save notes and web content quickly.",
  "permissions": ["contextMenus", "activeTab", "scripting", "storage"],
  
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.jpg"
  },
  "icons": {
    "128": "icons/icon.jpg"
  },
 
  "web_accessible_resources": [
  {
    "resources": ["icons/background.jpg"],
    "matches": ["<all_urls>"]
  }
]

  
}